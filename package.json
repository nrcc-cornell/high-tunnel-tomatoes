{
  "name": "high-tunnel-tomatoes",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "predeploy": "npm run build",
    "deploy": "git subtree push --prefix public/ origin gh-pages",
    "preghpagesdeploy": "npm run build",
    "ghpagesdeploy": "git subtree push --prefix public/ ghpages gh-pages",
    "check": "svelte-check",
    "prepare": "npm run smui-theme",
    "smui-theme": "smui-theme compile public/smui.css -i src/theme"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@smui/select": "^7.0.0-beta.15",
    "@smui/textfield": "^7.0.0-beta.15",
    "@tsconfig/svelte": "^3.0.0",
    "@types/leaflet": "^1.9.4",
    "chart.js": "^4.4.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "chartjs-plugin-annotation": "^3.0.1",
    "date-fns": "^2.30.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.9.0",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "sass": "^1.68.0",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^3.55.0",
    "svelte-chartjs": "^3.1.2",
    "svelte-check": "^3.0.0",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-material-icons": "^3.0.5",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0",
    "uuid": "^9.0.1"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  }
}
